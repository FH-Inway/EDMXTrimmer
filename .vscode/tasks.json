{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "clean",
        "command": "dotnet",
        "type": "process",
        "args": [
          "clean",
          "${workspaceFolder}/EDMXTrimmer/EDMXTrimmer/EDMXTrimmer.csproj"
        ]
      },
      {
        "label": "build",
        "command": "dotnet",
        "type": "process",
        "args": [
          "build",
          "${workspaceFolder}/EDMXTrimmer/EDMXTrimmer/EDMXTrimmer.csproj",
          "/property:GenerateFullPaths=true",
          "/consoleloggerparameters:NoSummary"
        ],
        "problemMatcher": "$msCompile"
      },
      {
        "label": "cleanbuild",
        "dependsOn": [
          "clean",
          "build"
        ]
      },
      {
        "label": "publish",
        "command": "dotnet",
        "type": "process",
        "args": [
          "publish",
          "${workspaceFolder}/EDMXTrimmer/EDMXTrimmer/EDMXTrimmer.csproj",
          "/property:GenerateFullPaths=true",
          "/consoleloggerparameters:NoSummary"
        ],
        "problemMatcher": "$msCompile"
      },
      {
        "label": "watch",
        "command": "dotnet",
        "type": "process",
        "args": [
          "watch",
          "run",
          "--project",
          "${workspaceFolder}/EDMXTrimmer/EDMXTrimmer/EDMXTrimmer.csproj"
        ],
        "problemMatcher": "$msCompile"
      }
    ]
  }